{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/api/v1/tool/crypt": {
            "post": {
                "description": "使用bcrypt加密串，返回hash值",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Tool"
                ],
                "summary": "获取Crypt Hash",
                "parameters": [
                    {
                        "description": "要Hash的串",
                        "name": "model",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/modules.CryptBindingModel"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Hash值",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "需要提供要Hash的串",
                        "schema": {
                            "$ref": "#/definitions/modules.ErrorViewModel"
                        }
                    }
                }
            }
        },
        "/api/v1/tool/uuid": {
            "get": {
                "description": "获取10个UUID，返回UUID列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Tool"
                ],
                "summary": "获取UUID",
                "responses": {
                    "200": {
                        "description": "UUID列表",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/tool/viewObjectId": {
            "post": {
                "description": "查看ObjectId信息，包括时间戳、MAC、PID和Counter",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Tool"
                ],
                "summary": "查看ObjectId信息",
                "parameters": [
                    {
                        "description": "要查看的ObjectId",
                        "name": "model",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/modules.ViewObjectIdBindingModel"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ObjectId信息",
                        "schema": {
                            "$ref": "#/definitions/modules.ViewObjectIdViewModel"
                        }
                    },
                    "400": {
                        "description": "需要提供有效的ObjectId",
                        "schema": {
                            "$ref": "#/definitions/modules.ErrorViewModel"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "modules.CryptBindingModel": {
            "type": "object",
            "required": [
                "str"
            ],
            "properties": {
                "str": {
                    "type": "string",
                    "example": "password"
                }
            }
        },
        "modules.ErrorViewModel": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string",
                    "example": "Error message"
                }
            }
        },
        "modules.ViewObjectIdBindingModel": {
            "type": "object",
            "required": [
                "objectId"
            ],
            "properties": {
                "objectId": {
                    "type": "string",
                    "example": "5ffd59f403762e429ea59a72"
                }
            }
        },
        "modules.ViewObjectIdViewModel": {
            "type": "object",
            "properties": {
                "counter": {
                    "type": "integer"
                },
                "machine": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "pid": {
                    "type": "integer"
                },
                "time": {
                    "type": "string"
                }
            }
        }
    }
}